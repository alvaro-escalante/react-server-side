{
  "name": "react-gulp",
  "version": "2.0.0",
  "license": "MIT",
  "description": "Gulp capabilities for house styles in React",
  "author": "Andy Sellick and Alvaro Fernandez",
  "main": "gulpfile.js",
  "scripts": {
    "upstart": "forever start -c node_modules/.bin/babel-node app/server.js",
    "run-npm-install": "npm install",
    "run-gulp": "gulp",
    "setup": "npm run run-npm-install && npm run run-gulp",
    "start": "cross-env NODE_ENV=production node_modules/.bin/babel-node --presets react,es2015 app/server.js",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-single-page": "node_modules/.bin/http-server app/static",
    "start-dev-hmr": "node_modules/.bin/webpack-dev-server --progress --inline --hot --open",
    "build": "cross-env NODE_ENV=production node_modules/.bin/webpack -p"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "devDependencies": {
    "babel": "^6.x.x",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.x.x",
    "babel-eslint": "^7.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.x.x",
    "babelify": "^7.3.0",
    "browser-sync": "^2.16.1",
    "browser-sync-reuse-tab": "^1.0.0",
    "browserify": "^13.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^3.2.4",
    "del": "^1.2.1",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-plugin-react": "^6.4.1",
    "eslintify": "^3.1.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-bytediff": "^0.2.1",
    "gulp-cache": "^0.2.10",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.1.7",
    "gulp-duration": "0.0.0",
    "gulp-htmlmin": "^3.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-load-plugins": "^1.3.0",
    "gulp-newer": "^0.5.2",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "handlebars": "^4.0.6",
    "react": "^15.3.2",
    "react-document-meta": "^2.1.1",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "stream-combiner2": "^1.1.1",
    "util-merge": "^1.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "express": "^4.15.2",
    "express-handlebars": "^3.0.0"
  }
}
