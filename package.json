{
  "name": "react-ssr-gulp",
  "version": "4.0.1",
  "license": "MIT",
  "description": "React - Server Side Render",
  "author": "Alvaro Fernandez",
  "main": "gulpfile.js",
  "scripts": {
    "setup": "npm run run-npm-install",
    "run-npm-install": "npm install",
    "run-gulp": "gulp",
    "start": "node_modules/.bin/babel-node app/server.js",
    "upstart": "forever start -c node_modules/.bin/babel-node app/server.js",
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "browserslist": [
    "iOS >= 8",
    "Chrome >= 30",
    "Explorer >= 11",
    "Last 2 Edge Versions",
    "Firefox >= 25"
  ],
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "add-module-exports",
      "react-html-attrs",
      "transform-react-display-name",
      "transform-class-properties"
    ],
    "ignore": [
      "app/static/assets/**",
      "app/static/css/**"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-react-html-attrs": "^2.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browser-sync": "^2.26.7",
    "browserify": "^16.3.0",
    "connect-history-api-fallback": "^1.3.0",
    "cross-env": "^3.2.4",
    "del": "^1.2.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-react": "^7.14.2",
    "eslintify": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-bytediff": "^1.0.0",
    "gulp-cache": "^1.1.2",
    "gulp-concat": "^2.6.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-data": "^1.3.1",
    "gulp-duration": "0.0.0",
    "gulp-exit": "0.0.2",
    "gulp-gzip": "^1.4.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^6.0.0",
    "gulp-load-plugins": "^2.0.0",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "handlebars": "^4.0.6",
    "react-addons-css-transition-group": "^15.4.2",
    "react-document-meta": "^2.1.1",
    "react-router": "^3.0.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1"
  },
  "dependencies": {
    "express": "^4.15.2",
    "natives": "^1.1.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-ga": "^2.1.2"
  }
}
